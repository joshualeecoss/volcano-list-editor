<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="455.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="question2.VolcanoMainController">
   <children>
      <TabPane fx:id="menuTabs" prefHeight="455.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="viewEditTab" text="View/Edit">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="467.0" prefWidth="815.0">
                     <children>
                        <fx:include source="ViewVolcano.fxml" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="addVolcanoTab" text="Add Volcano">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
