                        <fx:include source="AddVolcano.fxml" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="viewEditEruptionTab" text="Add/Delete Eruption">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
